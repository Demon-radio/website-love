{
  "version": 2,
  "builds": [
    {
      "src": "index.py",
      "use": "@vercel/python",
      "config": {
        "pip_path": "pip3",
        "pythonVersion": "3.9",
        "includeFiles": ["vercel_requirements.txt"],
        "installCommand": "pip3 install -r vercel_requirements.txt"
      }
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "index.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1"
  }
}